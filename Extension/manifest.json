{
  "name": "Gmail.js",
  "version": "0.1",
  "description": "Hello World Gmail chrome extension",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "utils/jquery-1.10.2.min.js",
    "utils/gmail.js",
    "main.js"
  ],
  "manifest_version": 2
}
